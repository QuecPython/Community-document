{
    "site_name": "QuecPython",
    "site_slogon": "让开发更迅速",
    "site_root_url": "/doc",
    "site_domain": "github.com/quecpython/wiki",
    "site_protocol": "https",
    "route": {
        "docs": {
            "/Quecpython_intro/zh": "docs/Quecpython_intro/zh",
            "/Development_env_tools/zh": "docs/Development_env_tools/zh",
            "/Quick_start/zh": "docs/Quick_start/zh",
            "/little_demo/zh": "docs/little_demo/zh",
            "/Advanced_development/zh": "docs/Advanced_development/zh",
            "/Product_case/zh": "docs/Product_case/zh",
            "/Mass_production/zh": "docs/Mass_production/zh",
            "/FAQ/zh": "docs/FAQ/zh",
            "/sbs/zh": "docs/sbs/zh"
        },
        "pages": {
            "/": "pages/index/zh"
        },
        "assets": {
            "/static/": "static"
        },
        "/blog/": "blog"
    },
    "executable": {
        "python": "python3",
        "pip": "pip3"
    },
    "plugins": {
        "teedoc-plugin-markdown-parser": {
            "from": "pypi",
            "config": {
                "toc_depth": 3
            }
        },
        "teedoc-plugin-theme-default": {
            "from": "pypi",
            "config": {
                "dark": true,
                "env": {
                    "main_color": "#DC143C"
                },
                "css": "/static/css/custom.css",
                "js": "/static/js/custom.js"
            }
        },
        "teedoc-plugin-search": {
            "from": "pypi",
            "config": {
                "search_hint": "Search"
            }
        }
    },
    "rebuild_changes_delay": 0,
    "robots": {
        "User-agent": "*"
    }
}